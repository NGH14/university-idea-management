{
	"compilerOptions": {
		"baseUrl": "src",
		"module": "commonjs",
		"target": "ESNext",
		"strict": true,
		"noEmit": false,
		"allowJs": true,
		"isolatedModules": true,
		"jsx": "react",
		"outDir": "./"
	},
	"exclude": [
		"node_modules",
		"./plugins/**/*",
		"./typings/**/*",
		"./built/**/*",
		"./src/App.js"
	],
	"include": [
		"src"
	]
}